{
  "version": 2,
  "builds": [
    {
      "src": "api/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/**",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ]
}
