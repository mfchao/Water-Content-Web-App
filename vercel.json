{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    },
    {
      "src": "api/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "status": 200
    }
  ]
}
